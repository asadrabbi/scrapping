{# templates/company/index.html.twig #}
<!DOCTYPE html>
<html>
<head>
    <title>Company List</title>
</head>
<body>
<h1>Company List</h1>
<table>
    <thead>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Registration Code</th>
        <th>VAT</th>
        <th>Address</th>
        <th>Mobile</th>
    </tr>
    </thead>
    <tbody>
    {% for company in companies %}
        <tr>
            <td>{{ company.id }}</td>
            <td>{{ company.title }}</td>
            <td>{{ company.registrationCode }}</td>
            <td>{{ company.vat }}</td>
            <td>{{ company.address }}</td>
            <td>{{ company.mobile }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
</body>
</html>
